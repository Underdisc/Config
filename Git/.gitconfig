[user]
	email = connortdeakin@gmail.com
	name = Underdisc
[core]
	autocrlf = false
[alias]
#STATUS COMMANDS
	s = status

#BRANCH COMMANDS
	snb = checkout -b
	sb = checkout
	b = branch
	rb = branch -r
	ab = branch -a
	fb = remote update
	m = merge

#COMMMIT COMMANDS
	cm = commit -m
	vommit = commit -m \":O~==~ This is broken\"

#PULL COMMANDS
	plo = pull origin
	pso = push origin

#LOG COMMANDS
	# single line
	lga = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
	lgs = !"git lga" -20
	lgl = !"git lga" -50
	# double line
	lgab = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
	lgsb = !"git lgab" -20
	lglb = !"git lgab" -50
